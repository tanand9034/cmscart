<!-- text -->
<div class="textm">
  <h2 style='text-align: center; margin-bottom: 50px;'>Daily needs</h2>
  <!-- cards start -->  

  <% products.forEach(function(p,i){ %>

    <% if (i % 4 == 0) { %>
      <div class="row mb20">
    <%} %>

      <div class="col-3">
        <div class="card" style="width: 18rem;">
          <img class="pimage" src="/product_images/<%= p.id %>/<%= p.image %>" alt="">
          <div class="card-body">
            <h5 class="card-title"><%= p.title %></h5>
            <p class="card-text">Rs.<%= parseFloat(p.price).toFixed(2) %></p>
            <a class="btn btn-primary" href="/products/<%= p.category %>/<%= p.slug %>">View Details</a>
          </div>
        </div>        
      </div>

    <% if (i % 4 == 3) { %>
      </div>
    <%} %>

  <% }); %>

</div>

